#IFFs
$Traitor IFF: Traitor
   
$IFF Name: Friendly
$Color: ( 128, 128, 255 )
$Attacks: ( "Hostile" "Neutral" "Killall" "Traitor" )
$Flags: ( "support allowed" )
$Default Ship Flags: ( "cargo-known" )
   	
